  Middleware (inspirado no rec4go)

  app.post('/alarms/:id/ack', {
    preHandler: [
      logEvent({
        eventType: EventType.ALARM_ACKNOWLEDGED,
        description: (req) => `Alarme ${req.params.id} reconhecido`,
        getResourceId: (req) => req.params.id,
        getMetadata: (req) => ({ acknowledgedBy: req.body.acknowledgedBy }),
      }),
    ],
  }, handler);

